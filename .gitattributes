  console.log("dfsd");
  alert("I am called from function inside,sayGreet");
}
sayGreet();
function sum() {
  let x = 30;
  let y = 25;
  console.log("Add x and y");
  console.log(x + y);
}
sum();
//Arguments
function login(email, password) {
  console.log("email", email);
  console.log("password,password");
  return email;
}
//Function Calling
//Parameters
let xyz = login("k@gmail.com", "12345");
console.log("xyz", xyz);
//Government
function checkAgeEligibility(age) {
  let isEligible = false;
  if (age >= 18) {
    isEligible = true;
  }
  return isEligible;
}
//My Own
function locallyCheckAgeEligibility(age) {
  let isEligible = checkAgeEligibility(age);
  if (isEligible == true) {
    alert("You are eligible");
  } else {
    alert("You are not eligible");
  }
}
console.log(checkAgeEligibility(3456));
